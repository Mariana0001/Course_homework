		{
			"name": "PT_Post_create_user_all_4_req",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"var jsonData = JSON.parse(responseBody);\r",
							"postman.setEnvironmentVariable(\"a1\", jsonData.age);\r",
							"postman.setEnvironmentVariable(\"temp_id\", jsonData.id);\r",
							"\r",
							"//let responseData = pm.response.json();\r",
							"//pm.environment.set(\"a1\",responseData.age);\r",
							"//pm.environment.set(\"temp_id\",responseData.id);\r",
							"\r",
							"pm.test(\"Response time is less than 1200ms\", function () {\r",
							"    pm.expect(pm.response.responseTime).to.be.below(1200);\r",
							"});\r",
							"\r",
							"let v1 = pm.variables.get(\"temp_id\");\r",
							"pm.test(\"User id is \"+ v1, function () {\r",
							"    pm.expect(pm.response.code).to.be.oneOf([200]);\r",
							"});\r",
							"\r",
							"pm.test(\"Successful POST request\", function () {\r",
							"    pm.expect(pm.response.code).to.be.oneOf([200, 201, 202]);\r",
							"});\r",
							"\r",
							"pm.test(\"Body matches string Ternopil\", function () {\r",
							"    pm.expect(pm.response.text()).to.include(\"Ternopil\");\r",
							"});\r",
							"\r",
							"\r",
							""
						],
						"type": "text/javascript"
					}
				}
			],
